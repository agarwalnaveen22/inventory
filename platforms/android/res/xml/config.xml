<?xml version='1.0' encoding='utf-8'?>
<widget id="com.inventory" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ActivityIndicator">
        <param name="android-package" value="org.apache.cordova.plugin.ActivityIndicator" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Inventory</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/ic_launcher.png" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="res/background.9.png" />
    <splash density="land-ldpi" src="res/background.9.png" />
    <splash density="land-mdpi" src="res/background.9.png" />
    <splash density="land-xhdpi" src="res/background.9.png" />
    <splash density="port-hdpi" src="res/background.9.png" />
    <splash density="port-ldpi" src="res/background.9.png" />
    <splash density="port-mdpi" src="res/background.9.png" />
    <splash density="port-xhdpi" src="res/background.9.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="splashScreenDelay" value="3000" />
</widget>
